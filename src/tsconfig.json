
{
  "extends": "../tsconfig.app.json",
  "compilerOptions": {
    "target": "ES2017", 
    "module": "ESNext",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": false
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
